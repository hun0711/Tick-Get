<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="back.spring.final_back.board.repository.TicketDao">
    <select id="getTicketList" resultType="TicketDto">
        select *
        from seoul_data
        where ticket Like "시민"
    </select>

</mapper>