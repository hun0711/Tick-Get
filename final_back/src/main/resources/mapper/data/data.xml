<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="back.spring.final_back.data.repository.DataDao">

    <select id="selectIdList" resultType="string">
      SELECT fest_m_id
        FROM fest_main
       ORDER BY fest_m_start desc
       LIMIT 500
    </select>

</mapper>